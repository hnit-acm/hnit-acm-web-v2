@import "func.styl"

for row in list
  un = row == auto ?:em
  size = row==auto ?auto:'%s%s'%(toFixed(row) un)
  dot = replace('\.','_',size)
  .padding-left-{dot}
    padding-left(size)
  .padding-right-{dot}
    padding-right(size)
  .padding-top-{dot}
    padding-top(size)
  .padding-bottom-{dot}
    padding-bottom(size)
